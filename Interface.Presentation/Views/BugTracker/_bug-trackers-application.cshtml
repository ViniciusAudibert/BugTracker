@model List<Interface.Presentation.Models.BugTracker.BugTrackerViewModel>

@foreach (var bug in Model)
{
    <tr>
        <td>
            <a data-toggle="modal" data-value="@bug.Trace" data-target="#modal" class="trace-link">
                @(bug.Trace.Length <= 15 ? @bug.Trace : @bug.Trace.Substring(0, 14))
            </a>
        </td>
        <td>@bug.Status</td>
        <td>@bug.OccuredDate.ToString("dd/MM/yyyy HH:mm")</td>
        <td>
            <a href="#" data-toggle="tooltip" data-placement="right" title="@bug.Tags">
                <i class="fa fa-tags table-icon" aria-hidden="true"></i>
            </a>
        </td>
        <td>@bug.Browser.Name</td>
        <td>@bug.Browser.Version</td>
        <td>@bug.OperationalSystem.Name</td>
    </tr>
}